rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    function signedIn() { return request.auth != null; }
    match /uploads/{uid}/{allPaths=**} {
      allow read: if request.auth.uid == uid;   // 要公開讀改成 true
      allow write: if signedIn() && request.auth.uid == uid;
    }
  }
}
